#   top-level Makefile for afnmr
install: 
	(cd blas && make install )
	(cd lapack && make install )
	(cd leap && make install )
	(cd etc && make install )
	(cd sff && make install )
	(cd MEAD && make install)
	(cd afnmr && make install)

clean::
	-(cd blas && make clean )
	-(cd lapack && make clean )
	-(cd leap && make clean )
	-(cd etc && make clean )
	-(cd sff && make clean )
	-(cd MEAD && make clean)
	-(cd afnmr && make clean)

